    "helmet": "^7.1.0",
    "multer": "^1.4.5-lts.1",
    "pdf-lib": "^1.17.1",
    "winston": "^3.11.0",
    "ytdl-core": "^4.11.2"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-security": "^1.7.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3"
